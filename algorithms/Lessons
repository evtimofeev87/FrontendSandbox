linearSearch(A,v)
    for i = 1 to A.length
        if A[i] === v
            return i
    return nil

binaryAdding(A,B)
    C = []
    cary = false
    for i = 1 to A.length
        C[i] = A[i] == B[i]
        if cary
            C[i] = !C[i]
        cary = A[i] & B[i]

